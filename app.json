{
  "expo": {
    "name": "spotify",
    "slug": "spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spotify_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotify_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "width": "150",
      "height": "150"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Spotify clone using react native typescript",
    "githubUrl": "https://github.com/watchDOGGGG/spotify.git"
  }
}
